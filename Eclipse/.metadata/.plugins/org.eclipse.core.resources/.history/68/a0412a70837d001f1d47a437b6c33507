package Methods;

public class ConstructorChain {
	String name;
	int age;
	long phno;
	double sal;
	String location;
	long tel;
	String blood;
	
	public ConstructorChain() {
	}
	
	ConstructorChain(String name, int age, long phno, double sal)
	{
		this.name = name;
		this.age = age;
		this.phno = phno;
		this.sal = sal;
	}
	
	ConstructorChain(String name, int age, long phno, double sal, String location)
	{
		
		this.name = name;
		this.age = age;
		this.phno = phno;
		this.sal = sal;
		this.location = location;
	}
	ConstructorChain(String name, int age, long phno, double sal, String location long tel)
	{
		
		this.name = name;
		this.age = age;
		this.phno = phno;
		this.sal = sal;
		this.location = location;
		this.tel = tel;
	}
	
	ConstructorChain(String name, int age, long phno, double sal, String location)
	{
		
		this.name = name;
		this.age = age;
		this.phno = phno;
		this.sal = sal;
		this.location = location;
	}
}
