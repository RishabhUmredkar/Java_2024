
public class Boxing {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		Integer i = new Integer(123);
		System.out.println(i);
		
		byte b1 = 5;
		Byte b = new Byte(b1);
		System.out.println(b);
		
		short st = 5;
		Short s = new Short(st);
		System.out.println(s);
	}

}
