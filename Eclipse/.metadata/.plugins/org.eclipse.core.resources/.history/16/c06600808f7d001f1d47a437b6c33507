package Encapsulation;

public class EmployeeDriver {

	public static void main(String[] args) {
			Employee emp = new Employee(101, "Rishabh" , "abc123", 2001, 300000,"analyst","Manager", "hyd", "QS");
			System.out.println(emp.getDeptName());
			emp.getId();
			emp.getDeptNo();
			emp.getJob();
			emp.getname();
			
	}

}
