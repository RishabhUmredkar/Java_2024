package Methods;

public class Statvar {
	static String s="stat";
	String ns = "nonstat var";
	public static void main(String[] args) {
		System.out.println(s);
		s = "hello";
		System.out.println(s);
		s = "hello1";
		System.out.println(s);
		test();
	}
	public static void test()
	{
		System.out.println(s + " test method");
		Statvar.s = "hii";
		System.out.println(s);
	}
}
