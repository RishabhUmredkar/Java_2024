package Methods;

public class blocks {
	blocks(){
		System.out.println("Constructor.....");
	}
	{
		System.out.println("Non static block");
	}
	public static void main(String[] args) {
		System.out.println("main block");
		blocks bs = new blocks();
	}
	
	
	static {
		System.out.println("Static block");
	}
}
